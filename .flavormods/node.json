{
	"tag_name": "node",
	"recipe_path": "$PKG_ROOT/pkg/flavors/node/",
	"doughmap": [
		{
			"key": "eslint",
			"type": "boolean",
			"default": false
		},
		{
			"key": "preset",
			"type": "list",
			"default": "cjs",
			"choices": [
				"cjs",
				"esm",
				"ts"
			]
		},
		{
			"key": "pkg_mgr",
			"type": "list",
			"default": "npm",
			"choices": [
				"npm",
				"yarn",
				"pnpm"
			]
		},
		{
			"key": "build_tools",
			"type": "list",
			"default": "none",
			"choices": [
				"none",
				"gulp",
				"esbuild",
				"grunt"
			]
		},
		{
			"key": "compiler",
			"type": "list",
			"default": "none",
			"choices": [
				"none",
				"babel",
				"swc",
				"esbuild"
			]
		},
		{
			"key": "bundler",
			"type": "list",
			"default": "none",
			"choices": [
				"none",
				"rollup",
				"webpack",
				"swcpack",
				"esbuild"
			]
		}
	]
}
